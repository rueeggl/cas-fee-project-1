{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
    "object-curly-newline": "off",
    "object-curly-spacing": "off",
    "linebreak-style": "off", 
    "no-unused-vars": "off", 
    "prefer-destructuring": "off",
    "no-undef": "off",
    "no-shadow": "off",
    "arrow-parens": "off", 
    "no-unused-expressions":"off", 
    "class-methods-use-this":"off", 
    "no-restricted-globals":"off", 
    "no-alert":"off", 
    "no-useless-concat":"off"



  }
}
